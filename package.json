{
    "name": "capstone-spa-router",
    "version": "0.0.0",
    "description": "Capstone project for Brandon Slade",
    "author": {
        "name": "Brandon Slade",
        "email": "brandonyoyoslade@gmail.com",
        "url": "http://www.bestoflies.com/"
    },
    "scripts": {
        "travis": "bash ./scripts/travis.sh",
        "deploy": "bash ./scripts/deploy.sh",
        "serve-all": "concurrently \"yarn serve-launchpad\" \"yarn serve-dep-loader-spike\" \"yarn serve-router-spike\" \"yarn serve-dependency-loader-visualizer\" \"yarn serve-router-demo\" \"yarn serve-programmatic-navigation-demo\" \"yarn serve-heavy-bundle-demo\" \"yarn serve-dependency-loader-visualizer-2\" \"yarn serve-route-parameter-demo\"",
        "serve-launchpad": "cd launchpad && yarn serve",
        "serve-dep-loader-spike": "cd dep-loader-spike && yarn serve",
        "serve-router-spike": "cd router-spike && yarn serve",
        "serve-dependency-loader-visualizer": "cd dependency-loader-visualizer && yarn serve",
        "serve-router-demo": "cd router-demo && yarn serve",
        "serve-programmatic-navigation-demo": "cd programmatic-navigation-demo && yarn serve",
        "serve-heavy-bundle-demo": "cd heavy-bundle-demo && yarn serve",
        "serve-dependency-loader-visualizer-2": "cd dependency-loader-visualizer-2 && yarn serve",
        "serve-route-parameter-demo": "cd route-parameter-demo && yarn serve",
        "build-all": "yarn build-launchpad && yarn build-dep-loader-spike && yarn build-router-spike && yarn build-dependency-loader-visualizer && yarn build-router-demo && yarn build-programmatic-navigation-demo && yarn build-heavy-bundle-demo && yarn build-dependency-loader-visualizer-2 && yarn build-route-parameter-demo",
        "build-launchpad": "cd launchpad && yarn build",
        "build-dep-loader-spike": "cd dep-loader-spike && yarn build",
        "build-router-spike": "cd router-spike && yarn build",
        "build-dependency-loader-visualizer": "cd dependency-loader-visualizer && yarn build",
        "build-router-demo": "cd router-demo && yarn build",
        "build-programmatic-navigation-demo": "cd programmatic-navigation-demo && yarn build",
        "build-heavy-bundle-demo": "cd heavy-bundle-demo && yarn build",
        "build-dependency-loader-visualizer-2": "cd dependency-loader-visualizer-2 && yarn build",
        "build-route-parameter-demo": "cd route-parameter-demo && yarn build"
    },
    "devDependencies": {},
    "dependencies": {
        "concurrently": "^3.5.0"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/blslade-neumont/capstone-spa-router.git"
    },
    "bugs": {
        "url": "https://github.com/blslade-neumont/capstone-spa-router/issues"
    },
    "homepage": "https://github.com/blslade-neumont/capstone-spa-router#readme"
}
